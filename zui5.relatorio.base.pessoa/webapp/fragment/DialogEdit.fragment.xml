                                <Input id="_IDGenInput1" value="{modelView/Idperson}"></Input>
                            </f:fields>
                        </f:FormElement>
                        <f:FormElement id="_IDGenFormElement2" label="{i18n>Fullname}">
                            <f:fields>
                                <Input id="_IDGenInput2" value="{modelView/Fullname}"></Input>
